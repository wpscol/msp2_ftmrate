{
  // Specify project name
  "name": "Development container - Ubuntu",
  // Specify target compose.yml with configuration
  "dockerComposeFile": "compose.yml",
  // Specify container to attach to
  "service": "devcontainer",
  // Specify mounted directory
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools-themes"
      ]
    }
  }
}